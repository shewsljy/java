<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 配置service
        <bean> 配置需要创建的对象
            id ：用于之后从spring容器获得实例时使用的
            class ：需要创建实例的全限定类名
    -->
    <bean id="helloJava" class="cn.jiayuli.spring.implement.HelloJava"></bean>
    <bean id="helloPython" class="cn.jiayuli.spring.implement.HelloPython"></bean>
    <bean id="helloService" class="cn.jiayuli.spring.services.HelloService">
        <property name="hello" ref="helloJava"></property>
    </bean>
</beans>